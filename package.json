{
  "name": "ember-qunit",
  "version": "0.1.8",
  "description": "QUnit helpers for testing Ember.js applications",
  "main": "lib/ember-qunit.js",
  "scripts": {
    "build": "rm -rf build && BROCCOLI_ENV=test broccoli build build",
    "prepublish": "bower install",
    "pretest": "npm run-script build",
    "test": "testem ci",
    "start": "BROCCOLI_ENV=test broccoli serve"
  },
  "authors": [
    "Stefan Penner",
    "Ryan Florence",
    "Robert Jackson",
    "Dan Gebhardt"
  ],
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.3.12",
    "broccoli": "^0.13.0",
    "broccoli-es6modules": "^0.4.0",
    "broccoli-funnel": "^0.1.6",
    "broccoli-jshint": "^0.5.3",
    "broccoli-merge-trees": "^0.1.4",
    "broccoli-sourcemap-concat": "^0.4.3",
    "broccoli-string-replace": "0.0.2",
    "git-repo-info": "^1.0.2",
    "testem": "0.6.22"
  }
}
